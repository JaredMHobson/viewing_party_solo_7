<div class="container text-center">
   <% if current_user %>
   <h2>Existing Users</h2>
      <ul id="existing_users">
         <% @users.each do |user| %>
            <li><%= user.email %></li>
         <% end %>
      </ul>
   <% end %>

   <%= button_to 'Create New User', register_user_path, method: :get, class: "btn btn-lg btn-outline-secondary" %><br>
</div>